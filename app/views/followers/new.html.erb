<%= form_tag(followers_new_path, :method => "get", id: "search-form") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search Users" %>
    <%= submit_tag "Search", :name => nil %>
<% end %>

<% if @searched_users.nil? %>
    no result
<%else%>
    <% @searched_users.each do |user| %>
        <%= user.username%>
        <%= user.email%>
        <br>
    <% end %>
<%end%>
